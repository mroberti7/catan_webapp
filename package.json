{
  "name": "catan_webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3007",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --detectOpenHandles --forceExit --passWithNoTests --coverage",
    "tsc-check": "tsc --noEmit",
    "prettier-check": "prettier --list-different --config=.prettierrc --ignore-path=.prettierignore .",
    "prettier-fix": "prettier --write --config=.prettierrc --ignore-path=.prettierignore  .",
    "precommit": "yarn prettier-check && yarn tsc-check",
    "generate-api": "openapi-generator-cli generate -i src/lib/catan-api-docs.json -g typescript-axios -o src/lib/generated"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@next/font": "^14.2.13",
    "@openapitools/openapi-generator-cli": "^2.13.12",
    "axios": "^1.7.7",
    "next": "14.2.13",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18",
    "eslint": "^9.11.1",
    "eslint-config-next": "14.2.13",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.2.5",
    "typescript": "^5"
  }
}
